<persistence xmlns="http://java.sun.com/xml/ns/persistence"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
        version="2.0">
    <persistence-unit name="jpamanager" transaction-type="RESOURCE_LOCAL">
    	<provider>org.hibernate.ejb.HibernatePersistence</provider>
    	<class>com.github.marcinseweryn.model.User</class>
    	<class>com.github.marcinseweryn.model.Doctor</class>
    	<class>com.github.marcinseweryn.model.WorkSchedule</class>
    	<class>com.github.marcinseweryn.model.Duty</class>
    	<class>com.github.marcinseweryn.model.Visit</class>
    	<class>com.github.marcinseweryn.model.Patient</class>
    	<class>com.github.marcinseweryn.model.PatientCard</class>
    </persistence-unit>
</persistence>